{
  "name": "dohtml",
  "version": "0.0.1",
  "description": "Simple utility to create HTML element by a string",
  "main": "index.js",
  "scripts": {
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "postversion": "npm run-script doc && git commit -a -m doc && npm run-script test && git push --follow-tags && npm publish",
    "test": "mocha test",
    "doc": "jsdoc2md --template README.hbs --no-gfm --files src/dohtml.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabioricali/dohtml.git"
  },
  "keywords": [
    "string-to-html",
    "dom"
  ],
  "author": "Fabio Ricali",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabioricali/dohtml/issues"
  },
  "homepage": "https://github.com/fabioricali/dohtml#readme",
  "devDependencies": {
    "bejs": "^2.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.1"
  }
}
